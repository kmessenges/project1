#include <fstream>
#include <iostream>
using namespace std;

int main() {
	char* header, *reader;
	int* a;
	float* b;
	header = new char[36];
	reader = new char[100];
	a = new int[10];
	b = new float[1];

	*header = 500;
	
	cout << &*header << endl;
	cout << header << endl;
	cout << header + 1 << endl;
	
	cout << reader << endl;
	cout << a << endl;
	cout << a + 1 << endl;
	cout << b << endl;
}
